<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gsrikar.riskcordovasample" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="FinBoxRiskManager">
        <param name="android-package" value="in.finbox.risk.FinBoxRiskManager" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FirebaseMessaging">
        <param name="android-package" value="by.chemerisuk.cordova.firebase.FirebaseMessagingPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>RiskCordovaSample</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="GradlePluginGoogleServicesEnabled" value="true" />
    <preference name="GradlePluginGoogleServicesVersion" value="4.4.0" />
</widget>
